<testSet xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.w3.org/XML/2004/xml-schema-test-suite/" xmlns:ts="http://www.w3.org/XML/2004/xml-schema-test-suite/" contributor="IBM" name="rf-whiteSpace" xsi:schemaLocation="http://www.w3.org/XML/2004/xml-schema-test-suite/ AnnotatedTSSchema.xsd">
  <annotation>
    <documentation xlink:href="http://www.w3.org/TR/xmlschema11-2/#rf-whiteSpace">collapse test for string.</documentation>
  </annotation>
  <testGroup name="d4_3_6v01" version="1.1">
    <annotation>
      <documentation>when the value is collapse, literals consisting solely of whitespace characters are reduced to the empty string.</documentation>
    </annotation>
    <documentationReference xlink:type="locator" xlink:href="http://www.w3.org/TR/xmlschema11-2/#rf-whiteSpace"/>
    <documentationReference xlink:type="locator" xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-Misc-CollapseEmptyString"/>
    <schemaTest name="d4_3_6v01s">
      <schemaDocument xlink:type="locator" xlink:href="../ibmData/valid/D4_3_6/d4_3_6v01.xsd"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-12-01"/>
    </schemaTest>
    <instanceTest name="d4_3_6v01i">
      <instanceDocument xlink:type="locator" xlink:href="../ibmData/valid/D4_3_6/d4_3_6v01.xml"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-12-01"/>
    </instanceTest>
  </testGroup>
  <testGroup name="d4_3_6ii01" version="1.1">
    <annotation>
      <documentation>when the value is collapse, literals consisting solely of whitespace characters are reduced to the empty string.</documentation>
    </annotation>
    <documentationReference xlink:type="locator" xlink:href="http://www.w3.org/TR/xmlschema11-2/#rf-whiteSpace"/>
