<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://www.w3.org/XML/2004/xml-schema-test-suite/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" contributor="IBM" name="TargetNS" version="1.1" xsi:schemaLocation="http://www.w3.org/XML/2004/xml-schema-test-suite/ ../common/xsts.xsd">
  <testGroup name="targetNamespace_004" version="1.1">
    <annotation>
      <documentation>
		    Composition of XML schemas via xs:import to be able to achive hetrogeneous namespaced elements
			in "one" XML document.
            </documentation>
    </annotation>
    <documentationReference xlink:type="locator" xlink:href="http://www.w3.org/TR/xmlschema11-1/"/>
    <documentationReference xlink:type="locator" xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-SchemaComposition-NSRefRequiresImport"/>
    <schemaTest name="targetNamespace_004_1">
      <schemaDocument xlink:type="locator" xlink:href="../ibmData/mixed/targetNamespace/tns4.xsd"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-10-12"/>
    </schemaTest>
    <instanceTest name="targetNamespace_004_2">
      <instanceDocument xlink:type="locator" xlink:href="../ibmData/mixed/targetNamespace/tns4.xml"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-10-12"/>
    </instanceTest>
  </testGroup>
  <testGroup name="targetNamespace_005" version="1.1">
    <annotation>
      <documentation>
		    This example demonstrates 'targetNamespace' attribute on xs:attribute schema component.
		    </documentation>
    </annotation>
    <documentationReference xlink:type="locator" xlink:href="http://www.w3.org/TR/xmlschema11-1/"/>
    <documentationReference xlink:type="locator" xlink:href="../common/XSD1_1TestCategories.xml#xsd1_1-NewLocalDecl-TargNSOnAttrDecl"/>
    <schemaTest name="targetNamespace_005_1">
      <schemaDocument xlink:type="locator" xlink:href="../ibmData/mixed/targetNamespace/tns5.xsd"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-10-12"/>
    </schemaTest>
    <instanceTest name="targetNamespace_005_2">
      <instanceDocument xlink:type="locator" xlink:href="../ibmData/mixed/targetNamespace/tns5.xml"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-10-12"/>
    </instanceTest>
  </testGroup>
</testSet>
