      <schemaDocument xlink:type="locator" xlink:href="../ibmData/instance_invalid/D4_3_6/d4_3_6ii01.xsd"/>
      <expected validity="valid"/>
      <current status="accepted" date="2010-12-01"/>
    </schemaTest>
    <instanceTest name="d4_3_6ii01i">
      <instanceDocument xlink:type="locator" xlink:href="../ibmData/instance_invalid/D4_3_6/d4_3_6ii01.xml"/>
